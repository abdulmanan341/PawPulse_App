{
  "version": 2,
  "builds": [
    {
      "src": "pages/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/assest/(.*)",
      "dest": "/assest/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/index.html"
    }
  ]
}
